                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
    <name>Upload file</name>
    <description>Action uploads file with product codes</description>
    <author>hybris</author>
    <version>1.0</version>

    <actionClassName>com.test.pic.backoffice.actions.UploadFileAction</actionClassName>

    <inputType>com.test.pic.core.model.SearchPicturesCronJobModel</inputType>
